<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textViewdevice"
        android:layout_width="207dp"
        android:layout_height="34dp"
        android:text="device"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewsensortx"
        android:layout_width="203dp"
        android:layout_height="32dp"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal|left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textViewsensor"
        app:layout_constraintTop_toBottomOf="@+id/textViewdevicetx" />

    <TextView
        android:id="@+id/textViewdevicetx"
        android:layout_width="202dp"
        android:layout_height="32dp"
        android:gravity="left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.936"
        app:layout_constraintStart_toEndOf="@+id/textViewdevice"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewunit"
        android:layout_width="208dp"
        android:layout_height="35dp"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal|left"
        android:text="unit"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewsensor" />

    <TextView
        android:id="@+id/textViewaddress"
        android:layout_width="202dp"
        android:layout_height="37dp"
        android:layout_marginStart="4dp"
        android:autoText="true"
        android:gravity="center|left"
        android:text="address"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewunit"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewsensor"
        android:layout_width="205dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal|left"
        android:text="sensor"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewdevice" />

    <TextView
        android:id="@+id/textViewunittx"
        android:layout_width="203dp"
        android:layout_height="37dp"
        android:gravity="center_horizontal|left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textViewunit"
        app:layout_constraintTop_toBottomOf="@+id/textViewsensortx" />

    <TextView
        android:id="@+id/textViewaddresstx"
        android:layout_width="199dp"
        android:layout_height="32dp"
        android:layout_marginTop="8dp"
        android:gravity="left"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toEndOf="@+id/textViewaddress"
        app:layout_constraintTop_toBottomOf="@+id/textViewunittx"
        app:layout_constraintVertical_bias="0.0" />

    <CheckBox
        android:id="@+id/checkBoxDE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewunittx" />

</androidx.constraintlayout.widget.ConstraintLayout>